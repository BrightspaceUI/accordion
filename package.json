{
  "description": "Polymer-based accordion component",
  "keywords": [
    "web-components",
    "d2l-collapse-button",
    "iron-collapse-button",
    "collapse",
    "button"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/accordion"
  },
  "homepage": "https://github.com/Brightspace/d2l-accordion",
  "name": "d2l-accordion",
  "scripts": {
    "start": "polymer serve",
    "test": "npm run test:lint && npm run test:polymer:local",
    "test:lint": "npm run test:lint:wc && npm run test:lint:js",
    "test:lint:js": "eslint . --ext .js,.html test/**/*.js test/**/*.html demo/**/*.js demo/**/*.html",
    "test:lint:wc": "polymer lint -i \"./*\" \"test\" \"demo\"",
    "test:polymer:local": "polymer test --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --skip-plugin local"
  },
  "author": "D2L",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/polyfill": "^7.0.0",
    "@polymer/iron-component-page": "^3.0.0-pre.18",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^4.15.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.1",
    "frau-ci": "^1.33.2",
    "polymer-cli": "^1.9.2",
    "wct-browser-legacy": "^1.0.1"
  },
  "version": "1.2.0",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  },
  "dependencies": {
    "d2l-colors": "BrightspaceUI/colors#semver:^4",
    "d2l-icons": "BrightspaceUI/icons#semver:^6",
    "@polymer/iron-collapse": "^3.0.0-pre.18",
    "@polymer/iron-flex-layout": "^3.0.0-pre.18",
    "@polymer/iron-selector": "^3.0.0-pre.18",
    "@polymer/polymer": "^3.0.0",
    "d2l-polymer-behaviors": "Brightspace/d2l-polymer-behaviors-ui#semver:^2"
  }
}
